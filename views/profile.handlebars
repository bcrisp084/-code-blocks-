<div class="top-bar" id="profile-nav">
  <div class="top-bar-right">
    <ul class="menu">
      <li><a class="my-uploads" href="">My uploads</a></li>
      <li><a class="my-faves" href="">Favorites</a></li>
      <li><a href="/home">Code Blocks</a></li>
    </ul>
  </div>
</div>

<div class="grid-y medium-grid-frame">
  <div class="cell shrink header medium-cell-block-container">
    <div class="grid-x grid-padding-x">
      <div class="cell medium-4">
        <img
          src="images/profile-pic.jpg"
          alt=""
          width="100px"
          height="100px"
          style="border-radius: 50%;"
        />
        <h1>{{username}}
        </h1>
      </div>

    </div>
  </div>
  <div class="cell medium-auto medium-cell-block-container">
    <div class="grid-x grid-padding-x">
      <div class="cell medium-4 medium-cell-block-y">
        <div>
          <button class="button" type="button">Edit profile</button>
        </div>
        <p>I work as a teaching assistant for EDx</p>
        <p>I am a proud father of three.</p>
        <p>I love to code and work with my hands.</p>
        <form
          class="image-upload"
          enctype="multipart/form-data"
          action="/api/image/form"
          method="post"
        >
          <label for="photo">Browse photos...</label>
          <input type="file" name="photo" id="photo" accept="images/*" />
          <label for="description"></label>
          <input
            type="text"
            class="description"
            id="description"
            name="description"
          />
          <button class="button" type="submit" id="photo-submit">submit</button>
        </form>

      </div>
      <div class="cell medium-8 medium-cell-block-y">
        <div  id="profile-uploads">
              {{> profile-uploads}}
        </div>
        <div class="display-type" id="favorites">
          {{> favorites}}
        </div>
      </div>
    </div>
  </div>
  <div class="cell shrink footer">
    <h3>Code Blocks</h3>
  </div>
</div>
